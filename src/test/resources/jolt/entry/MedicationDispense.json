					}
				}
			},
			"status": "=valueSetAdapter('MedicationDispenseStatus',@0)",
			"whenPrepared": "=datetimeAdapter(@0)",
			"whenHandedOver": "=datetimeAdapter(@0)",
			"dosageInstruction": {
				"*": {
